#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/meta-share/lexmeta/dictionaryAccessType
lexmeta:dictionaryAccessType rdf:type owl:ObjectProperty ;
                             rdfs:domain ms:LexicalConceptualResource ;
                             rdfs:range [ owl:intersectionOf ( skos:Concept
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty skos:inScheme ;
                                                                 owl:hasValue lexmeta:AccessStructureTypeScheme
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                             rdfs:comment "Classifies the dictionary based on the approach (onomasiological vs. semasiological) adopted for the organization of the lexical entries"@en ;
                             rdfs:label "dictionary access type"@en .


###  http://w3id.org/meta-share/lexmeta/dictionaryFunctionType
lexmeta:dictionaryFunctionType rdf:type owl:ObjectProperty ;
                               rdfs:domain ms:LexicalConceptualResource ;
                               rdfs:range [ owl:intersectionOf ( skos:Concept
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty skos:inScheme ;
                                                                   owl:hasValue lexmeta:DictionaryFunctionTypeScheme
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                               rdfs:comment "Classifies the dictionary in terms of its intended function (e.g., text translation, vocabulary learning) according to the Theory of Lexicographical Functions"@en ;
                               rdfs:label "ditionary function type"@en .


###  http://w3id.org/meta-share/lexmeta/dictionaryScope
lexmeta:dictionaryScope rdf:type owl:ObjectProperty ;
                        rdfs:domain ms:LexicalConceptualResource ;
                        rdfs:range [ owl:intersectionOf ( skos:Concept
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty skos:inScheme ;
                                                            owl:hasValue lexmeta:DictionaryScopeTypeScheme
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
                        rdfs:comment "Classifies the dictionary in terms of its contents and intended use, based mainly on the classification proposed by Engelberg & Storrer (Typologie von Internetwörterbüchern und -portalen, 2016)"@en ;
                        rdfs:label "dictionary scope"@en .


###  http://w3id.org/meta-share/lexmeta/dictionaryTextPart
lexmeta:dictionaryTextPart rdf:type owl:ObjectProperty ;
                           rdfs:domain ms:LexicalConceptualResource ;
                           rdfs:range [ owl:intersectionOf ( skos:Concept
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty skos:inScheme ;
                                                               owl:hasValue lexmeta:DictionaryTextPartScheme
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                           rdfs:comment "Specifies text parts present in the dictionary text (such as front or back matters, and entry types)"@en ;
                           rdfs:label "dictionary text part"@en .


###  http://w3id.org/meta-share/lexmeta/lemmaType
lexmeta:lemmaType rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain ms:LexicalConceptualResource ;
                  rdfs:range [ owl:intersectionOf ( skos:Concept
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty skos:inScheme ;
                                                      owl:hasValue lexmeta:LemmaTypeScheme
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
                  rdfs:comment "Specifies the types of headwords included in a dictionary (e.g., whether it includes single- or multi-word units, abbreviations, neologisms, etc.)"@en ;
                  rdfs:label "lemma type"@en .


###  http://w3id.org/meta-share/lexmeta/lexicographicalProcessType
lexmeta:lexicographicalProcessType rdf:type owl:ObjectProperty ;
                                   rdfs:domain ms:LexicalConceptualResource ;
                                   rdfs:range [ owl:intersectionOf ( skos:Concept
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty skos:inScheme ;
                                                                       owl:hasValue lexmeta:LexicographicalProcessTypeScheme
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                                   rdfs:comment "Provides information on how the dictionary was created (e.g., in a collaborative mode, involving academic lexicographers vs. citizens, etc.)"@en ;
                                   rdfs:label "lexicographical process type"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty .


